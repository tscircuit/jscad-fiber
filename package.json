{
  "name": "jscad-fiber",
  "version": "0.0.93",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/jscad-fiber"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./three": {
      "import": "./dist/three.js",
      "types": "./dist/three.d.ts"
    }
  },
  "scripts": {
    "start": "cosmos",
    "build": "npm run build:lib",
    "build:lib": "tsup ./lib/index.tsx ./lib/three.tsx --dts --sourcemap --format esm",
    "build:cosmos": "cosmos-export;cp -r examples cosmos-export;mv cosmos-export/examples cosmos-export/code-examples",
    "predeploy": "BUILD_FOR_GH_PAGES=1 npm run build:cosmos && touch ./cosmos-export/.nojekyll",
    "format": "bun x biome format . --write",
    "format:check": "bun x biome format ."
  },
  "peerDependencies": {
    "@jscad/modeling": "*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-reconciler": "*",
    "three": "*"
  },
  "peerDependenciesMeta": {
    "@jscad/modeling": {
      "optional": true
    },
    "three": {
      "optional": true
    }
  },
  "dependencies": {
    "color": "^4.2.3"
  },
  "devDependencies": {
    "react-code-blocks": "^0.1.6",
    "lucide-react": "^0.525.0",
    "@biomejs/biome": "^1.9.4",
    "@jscad/modeling": "^2.12.2",
    "@react-three/drei": "10.6.1",
    "@react-three/fiber": "9.3.0",
    "@types/bun": "^1.1.14",
    "@types/react": "19.0.1",
    "@types/three": "^0.166.0",
    "@vitejs/plugin-react": "^5.0.0",
    "gh-pages": "^5.0.0",
    "jscad-planner": "^0.0.10",
    "react-cosmos": "^6.1.1",
    "react-cosmos-plugin-vite": "^6.1.1",
    "three": "^0.166.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
